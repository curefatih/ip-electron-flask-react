    "typescript": "^4.0.5"
  },
  "dependencies": {
    "electron-is-dev": "^1.2.0",
    "isomorphic-fetch": "^3.0.0",
    "python-shell": "^2.0.2",
    "uuid": "^8.3.1"
  }
